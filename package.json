{
  "name": "dxsix-form-components",
  "version": "0.1.12",
  "private": false,
  "author": "dxsixpc",
  "license": "MIT",
  "homepage": "https://github.com/dxsixpc/dxsix-form-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxsixpc/dxsix-form-components.git"
  },
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "module": "dist/index.modern.js",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "storybook",
    "typescript"
  ],
  "scripts": {
    "start": "run-s storybook",
    "rollup-build": "rollup -c -w",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "lint": "run-s lint:js lint:style lint:prettier",
    "lint-staged:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:prettier": "prettier --check --write \"**/*.{tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:style": "stylelint --aei --fix \"src/**/*.less\" --syntax less",
    "prepublishOnly": "run-s lint test build",
    "test": "jest --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "ahooks": "^2.10.9",
    "antd": "^4.16.13",
    "axios": "^0.21.4",
    "braft-editor": "^2.3.9",
    "lodash": "^4.17.21",
    "microbundle-crl": "^0.13.11",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-sortablejs": "1.5.1",
    "sortablejs": "^1.14.0",
    "styled-components": "^5.3.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.173",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^17.0.0",
    "@types/sortablejs": "^1.10.7",
    "@types/styled-components": "^5.1.14",
    "babel-loader": "^8.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "core-js": "^3.19.1",
    "eslint-plugin-yml": "^0.11.0",
    "jest": "26.6.0",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "stylelint": "^13.0.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "webpack": "^4.44.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix --syntax less",
    "**/*.{ts,tsx}": "yarn run lint-staged:js",
    "**/*.{tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ]
}
